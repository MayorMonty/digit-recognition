{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "lib": ["es6", "dom", "es2017", "esnext"],
    "downlevelIteration": true,
    "strictNullChecks": false,
    "skipLibCheck": false,
    "strict": false,
    "allowJs": true
  },
  "exclude": ["node_modules"]
}
